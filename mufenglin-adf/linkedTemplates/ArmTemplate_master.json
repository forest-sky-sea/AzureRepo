{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mufenglin-adf"
		},
		"mufenglin_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mufenglin_AzureBlobStorage'"
		},
		"mufenglin_AzureHDInsight4_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'mufenglin_AzureHDInsight4'"
		},
		"mufenglin_AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'mufenglin_AzureDatabricks'"
		},
		"mufenglin_AzureHDInsight4_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mufenglin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mufenglin-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"mufenglin_AzureBlobStorage_connectionString": {
						"value": "[parameters('mufenglin_AzureBlobStorage_connectionString')]"
					},
					"mufenglin_AzureDatabricks_accessToken": {
						"value": "[parameters('mufenglin_AzureDatabricks_accessToken')]"
					},
					"mufenglin_AzureHDInsight4_password": {
						"value": "[parameters('mufenglin_AzureHDInsight4_password')]"
					},
					"mufenglin_AzureHDInsight4_properties_typeProperties_userName": {
						"value": "[parameters('mufenglin_AzureHDInsight4_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}